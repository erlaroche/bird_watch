h1 {
  font-family: 'Galindo', cursive;
  text-shadow: 2px 2px #8080B2;
}
h2 {
  font-family: 'Galindo', cursive;
  color: black;
  text-shadow: 2px 2px #8080B2;
}

h3 {
  color: black;
  font-family: 'Galindo', cursive;
  font-size: 30px;
}

h4 {
  color: black;
  font-family: 'Galindo', cursive;
}

.entries {
  background-color: #8080B2;
  border:2px solid;
  border-radius:25px; 
}

.button {
  padding: 10px 15px;
  border-radius: 4px;
  border: 1px solid #20538D;
  color: #00003D;
  background-color: #8080B2;
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.4), 0 1px 1px rgba(0, 0, 0, 0.2);
  font-family: 'Galindo', cursive;
  text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.4);
  font-size: 20px;
  text-transform: uppercase;
  display: block;
  margin-top: 5px;
  margin-bottom: 5px;
}